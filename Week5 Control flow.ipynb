    "def pay_computation(hours, rate):\n",
    "    if hours>40:\n",
    "        salary = ((hours - 40)* rate*1.5)+(40*rate)\n",
    "        \n",
    "    else:\n",
    "        salary = 40*rate\n",
    "    return salary"
